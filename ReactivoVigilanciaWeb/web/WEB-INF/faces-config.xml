<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.0"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_0.xsd">
    <application>
        <message-bundle>
            co.gov.invima.mensajes.messages
        </message-bundle>
    </application>
    
    <!-- *************************************************************** -->
    <!-- *************************************************************** -->
    <!-- *************************************************************** -->
    <managed-bean>
        <managed-bean-name>loginBean</managed-bean-name>
        <managed-bean-class>co.gov.invima.beans.LoginBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <!-- *************************************************************** -->
    <!-- *************************************************************** -->
    <!-- *************************************************************** -->
    <managed-bean>
        <managed-bean-name>inscripcionBean</managed-bean-name>
        <managed-bean-class>co.gov.invima.beans.InscripcionBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <!-- *************************************************************** -->
    <!-- *************************************************************** -->
    <!-- *************************************************************** -->
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>inscripcion</from-outcome>
            <to-view-id>/faces/inscripcion.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <!-- *************************************************************** -->
    <!-- *************************************************************** -->
    <!-- *************************************************************** -->
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>login</from-outcome>
            <to-view-id>/faces/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <!-- ************************************************************************** -->
    <!-- ************************************************************************** -->
    <!-- ************************************************************************** -->
        <lifecycle>
            <phase-listener id="nocacheReactivo">co.gov.invima.util.CacheControlPhaseListenerReactivo</phase-listener>
        </lifecycle>
    <!-- ************************************************************************** -->
    <!-- ************************************************************************** -->
    <!-- ************************************************************************** -->
</faces-config>
